# Neovim vterm terminal escape sequences
# CSI (Control Sequence Introducer)
"\x1b["
# OSC (Operating System Command)
"\x1b]"
# DCS (Device Control String)
"\x1bP"
# APC (Application Program Command)
"\x1b_"
# PM (Privacy Message)
"\x1b^"
# SOS (Start of String)
"\x1bX"
# ST (String Terminator)
"\x1b\\"
# SS2, SS3
"\x1bN"
"\x1bO"
# BEL (alternative ST)
"\x07"
# Common CSI sequences
"\x1b[m"
"\x1b[0m"
"\x1b[1m"
"\x1b[4m"
"\x1b[7m"
"\x1b[H"
"\x1b[J"
"\x1b[K"
"\x1b[2J"
"\x1b[?25h"
"\x1b[?25l"
"\x1b[?1049h"
"\x1b[?1049l"
# SGR (Select Graphic Rendition) parameters
"\x1b[38;5;"
"\x1b[48;5;"
"\x1b[38;2;"
"\x1b[48;2;"
# Cursor movement
"\x1b[A"
"\x1b[B"
"\x1b[C"
"\x1b[D"
# Scroll
"\x1b[S"
"\x1b[T"
